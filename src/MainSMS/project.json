{
  "title": "MainSMS",
  "version": "1.1.0",
  "description": "C# services for MainSMS.ru API",

  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/kroniak/extensions-mainsms/master/LICENSE",
    "projectUrl": "https://github.com/kroniak/extensions-mainsms",
    "releaseNotes": "https://github.com/kroniak/extensions-mainsms/releases",
    "repository": {
      "type": "git",
      "url": "https://github.com/kroniak/extensions-mainsms.git"
    },
    "requireLicenseAcceptance": false,
    "summary": "C# services for MainSMS.ru API",
    "tags": [ "mainsms sms api client rest async" ]
  },

  "authors": [ "me@kroniak.net" ],
  "copyright": "Copyright Â©2016 by me@kroniak.net",
  "license": "GPL-3.0",

  "buildOptions": {
    "xmlDoc": true,
    "warningsAsErrors": true
  },

  "configurations": {
    "Release": {
      "buildOptions": {
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Flurl.Http.Xml": "1.3.0"
  },

  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "NET45" ]
      },
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0"
      }
    },
    "netstandard1.6": {
      "buildOptions": {
        "define": [ "NETSTD" ]
      },
      "dependencies": {
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Text.Encodings.Web": "4.0.0"
      }
    }
  }
